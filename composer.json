{
    "name": "mingalevme/illuminate-google",
    "description": "Provides Google API Library for PHP wrapper for Laravel/Lumen",
    "keywords": [
        "laravel",
        "lumen",
        "google",
        "google-api"
    ],
    "homepage": "https://github.com/mingalevme/illuminate-google",
    "license": "MIT",
    "authors": [
        {
            "name": "Mingalev Mikhail",
            "email": "mingalevme@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "vlucas/phpdotenv": ">=2.2",
        "google/apiclient": "^2.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "php": ">=7.1.0",
        "laravel/lumen": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "laravel/laravel": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0",
        "mockery/mockery": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "Mingalevme\\Illuminate\\Google\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mingalevme\\Tests\\Illuminate\\Google\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
